   "source": [
    "字典是可变对象,所以`=`是浅拷贝\n",
    "\n",
    "`setdefault()`用法"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 14,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "{'a': {'b': 1}}\n",
      "{'b': 1}\n",
      "{'a': {'b': 0}}\n",
      "{'b': 0}\n"
     ]
    }
   ],
   "source": [
    "p = {}\n",
    "a = p.setdefault('a', {})\n",
    "a['b'] = 1\n",
    "print(p)\n",
    "aa = p.setdefault('a',{})\n",
    "#aa = p['a']\n",
    "print(aa)\n",
    "aa['b'] = 0\n",
    "print(p)\n",
    "print(a)"
   ]
  },
